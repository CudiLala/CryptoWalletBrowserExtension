{
  "name": "mola-wallet-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prepare-chrome": "cp ./manifest/chrome.json ./public/manifest.json",
    "prepare-firefox": "cp ./manifest/firefox.json ./public/manifest.json",
    "lint:build": "mv ./out/_next ./out/next && cd ./out && grep -rl '/_next' * | xargs sed -i 's|/_next|/next|g'",
    "lint:build-osx": "mv ./out/_next ./out/next && cd ./out && grep -rli '_next' * | xargs -I@ sed -i '' 's|/_next|/next|g' @;",
    "build": "next build && next export && npm run lint:build",
    "build-osx": "next build && next export && npm run lint:build-osx",
    "build-chrome": "npm run prepare-chrome && npm run build",
    "build-chrome-osx": "npm run prepare-chrome && npm run build-osx",
    "build-firefox": "npm run prepare-firefox && npm run build",
    "build-firefox-osx": "npm run prepare-firefox && npm run build-osx",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ethersproject/units": "^5.7.0",
    "@moralisweb3/evm-utils": "^2.7.4",
    "@next/font": "13.1.6",
    "@types/ethereum-blockies": "^0.1.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.1.3",
    "bip39": "^3.0.4",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "ethereum-blockies": "^0.1.1",
    "ethereumjs-wallet": "^1.0.2",
    "js-file-download": "^0.4.12",
    "moralis": "^2.7.4",
    "nanoid": "^4.0.0",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5",
    "web3": "^1.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
